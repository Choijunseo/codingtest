from itertools import combinations
def solution(nums):
    nums2=list(set(nums))
    answer=len(nums2) if len(nums2)<int(len(nums)/2) else int(len(nums)/2)
    #lst=list(combinations(nums,int(len(nums)/2)))
    #answer=max([len(set(i)) for i in lst])
    return answer
